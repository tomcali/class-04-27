<style media="screen">
  li {
    height : 100px;
    width: 100px;
    color: red;
    background-color: purple;
    margin-bottom: 30px;
  }
</style>

<h1>Lunches for {{eater}}</h1>

<!-- like a for-loop... but using template version each --->
<ul>
{{#each foods }}
  <li>{{lunch}}</li>  <!-- could have said this.lunch showing that it is an object ---->
{{/each}}
</ul>
